REQ joined_ack(){
    REQ r;
    r.reqtype= JOINED_ACK;
    r.sender= id;
    r.front= front; 
    r.spacing= 0;
    r.steering= false; 
    return r;
}

REQ join_req(){
    REQ r;
    r.reqtype= JOIN_REQ;
    r.sender= id;
    if(id == 3){
    r.front= 2; }
    else{
    r.front= 3;
    }
    return r;
}

REQ left_ack(){
    REQ r;
    r.reqtype= LEFT_ACK;
    r.sender= id;
    return r;
}

REQ leave_req(){
    REQ r;
    r.reqtype= LEAVE;
    r.sender= id;
    return r;
}

